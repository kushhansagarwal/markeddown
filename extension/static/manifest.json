{
  "name": "MarkedDown",
  "description": "Identify illegal digital media",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": ["activeTab"],
  "action": {
    "default_title": "Svelte Extension",
    "default_icon": "favicon.png",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}
